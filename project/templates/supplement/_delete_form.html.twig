<form method="post" action="{{ path('app_supplement_delete', {'id': supplement.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ supplement.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
